<?xml version="1.0" encoding="utf-8"?>
    <shape xmlns:android="http://schemas.android.com/apk/res/android">
        <!-- Thiết lập màu nền cho button -->
        <solid android:color="#66CC99" />

        <!-- Thiết lập góc tròn (để tạo hình tròn) -->
        <corners android:radius="100dp" />

        <!-- Thiết lập kích thước viền cho button (nếu có) -->
        <stroke
            android:width="2dp"
            android:color="#FFFFFF" />
    </shape>